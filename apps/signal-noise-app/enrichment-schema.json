  "enrichment_rules": {
    "priority_scoring": {
      "high_priority": {
        "criteria": ["opportunity_score >= 80", "relationship_score >= 70"],
        "refresh_frequency": "daily",
        "data_sources": ["brightdata", "perplexity"]
      },
      "medium_priority": {
        "criteria": ["opportunity_score >= 50", "relationship_score >= 50"],
        "refresh_frequency": "weekly",
        "data_sources": ["brightdata", "perplexity"]
      },
      "low_priority": {
        "criteria": ["opportunity_score < 50", "relationship_score < 50"],
        "refresh_frequency": "monthly",
        "data_sources": ["perplexity"]
      }
    },
    "data_quality_rules": {
      "required_fields": ["name", "type"],
      "email_validation": "regex_pattern",
      "url_validation": "url_format",
      "phone_validation": "phone_format",
      "date_validation": "iso_date_format"
    },
    "data_sources": {
      "brightdata": {
        "company_research": "https://api.brightdata.com/v1/companies/domain",
        "person_search": "https://api.brightdata.com/v1/people/search",
        "news_monitoring": "https://api.brightdata.com/v1/news/search",
        "market_intelligence": "https://api.brightdata.com/v1/markets/analysis"
      },
      "perplexity": {
        "market_analysis": "Industry trends and competitive positioning",
        "strategic_insights": "Business strategy and innovation focus",
        "financial_intelligence": "Performance metrics and investment patterns",
        "relationship_mapping": "Partnership networks and competitive relationships"
      }
    }
  },

  "neo4j_schema": {
    "indexes": [
      "CREATE INDEX entity_name_index IF NOT EXISTS FOR (e:Entity) ON (e.name)",
      "CREATE INDEX entity_type_index IF NOT EXISTS FOR (e:Entity) ON (e.type)",
      "CREATE INDEX person_email_index IF NOT EXISTS FOR (p:Person) ON (p.email)",
      "CREATE INDEX club_website_index IF NOT EXISTS FOR (c:Club) ON (c.website)",
      "CREATE INDEX organization_industry_index IF NOT EXISTS FOR (o:Organization) ON (o.industry)"
    ],
    "vector_index": [
      "CREATE VECTOR INDEX entity_embeddings IF NOT EXISTS FOR (n:Entity) ON n.embedding OPTIONS { indexConfig: { `vector.dimensions`: 1536, `vector.similarity_function`: 'cosine' } }"
    ],
    "fulltext_index": [
      "CREATE FULLTEXT INDEX entity_search_index IF NOT EXISTS FOR (e:Entity) ON EACH [e.name, e.description, e.type, e.sport, e.country]"
    ],
    "constraints": [
      "CREATE CONSTRAINT entity_unique IF NOT EXISTS FOR (e:Entity) REQUIRE e.name IS UNIQUE",
      "CREATE CONSTRAINT person_email_unique IF NOT EXISTS FOR (p:Person) REQUIRE p.email IS UNIQUE"
    ]
  },

