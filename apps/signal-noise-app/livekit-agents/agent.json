{
  "name": "yellow-panther-voice-agent",
  "description": "Voice AI Agent for Yellow Panther Signal Noise App with Claude Agent integration",
  "runtime": "nodejs18",
  "entry_point": "agent.js",
  "environment": {
    "LIVEKIT_URL": "wss://yellow-panther-8i644ma6.livekit.cloud",
